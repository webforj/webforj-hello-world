{
  "name": "Webforj HelloWorld Application",
  "forwardPorts": [6065],
  "image": "mcr.microsoft.com/vscode/devcontainers/java:17",
  "portsAttributes": {
    "6065": {
      "label": "App Preview",
      "onAutoForward": "notify"
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/maven:1": {
      "version": "3.8.6" // You can specify the desired Maven version here
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "BEU.vscode-java-html",
        "vscjava.vscode-maven"
      ]
    }
  }
}
